<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<ThingDef ParentName="PlantBase">
		<defName>RG-AF_Plant_AspenGrass</defName>
		<label>grass</label>
		<description>Wild grass. Grows in aspen forest regions where there is a little light and minimally fertile ground.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Flammability>1.2</Flammability>
			<Nutrition>0.20</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/AspenGrass</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.3</fertilitySensitivity>
			<growDays>2.5</growDays>
			<leaflessGraphicPath>Things/Plant/Grass_Leafless</leaflessGraphicPath>
			<harvestWork>40</harvestWork>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.4~0.6</visualSizeRange>
			<topWindExposure>0.4</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>

	<ThingDef ParentName="BushBase">
		<defName>RG-AF_Plant_AspenFern</defName>
		<label>fern</label>
		<description>A short shrub with long, fine leaves. Grows in forests in aspen forests.</description>
		<graphicData>
			<texPath>Things/Plant/AspenFern</texPath>
			<drawSize>1.15</drawSize>
			<shadowData>
				<volume>(0.55, 0.2, 0.2)</volume>
			</shadowData>
		</graphicData>
		<uiIconScale>0.85</uiIconScale>
		<plant>
			<growDays>3</growDays>
			<leaflessGraphicPath>Things/Plant/Leafless/AspenFern_Leafless</leaflessGraphicPath>
			<harvestWork>60</harvestWork>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
		<defName>RG-AF_Plant_AspenShrubs</defName>
		<label>shrubs</label>
		<description>Wild carpet shrubs with yellow leafs that grows in aspen forests. Shrubs takes a long time to grow, but is very frost-resistant.</description>
		<statBases>
			<MaxHitPoints>120</MaxHitPoints>
			<Flammability>0.6</Flammability>
			<Nutrition>0.15</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/AspenShrubs</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.5</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilityMin>0.05</fertilityMin>
			<fertilitySensitivity>0.2</fertilitySensitivity>
			<growDays>15</growDays>
			<leaflessGraphicPath>Things/Plant/Leafless/AspenShrubs_Leafless</leaflessGraphicPath>
			<harvestWork>75</harvestWork>
			<maxMeshCount>4</maxMeshCount>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<visualSizeRange>0.5~0.8</visualSizeRange>
			<topWindExposure>0.3</topWindExposure>
			<wildOrder>1</wildOrder>
		</plant>
	</ThingDef>

	<ThingDef ParentName="DeciduousTreeBase">
		<defName>RG-AF_Plant_TreeAspenRed</defName>
		<label>aspen tree</label>
		<description>A genetically modified cold-biome tree known for its thin, white, paper-like bark and colourful leaves.</description>
		<graphicData>
			<texPath>Things/Plant/TreeRedAspen</texPath>
			<graphicClass>Graphic_Random</graphicClass>
			<shadowData>
				<volume>(0.15, 0.3, 0.1)</volume>
				<offset>(-0.03,0,-0.44)</offset>
			</shadowData>
		</graphicData>
		<statBases>
			<Beauty>2</Beauty>
			<Nutrition>1.25</Nutrition>
		</statBases>
		<ingestible/>
		<plant>
			<growDays>20</growDays>
			<harvestWork>800</harvestWork>
			<harvestYield>27</harvestYield>
			<leaflessGraphicPath>Things/Plant/TreeBirch_Leafless</leaflessGraphicPath>
		</plant>
		<comps>
			<li Class="CompProperties_Spawner">
				<compClass>VFECore.CompLeavesSpawner</compClass>
				<thingToSpawn>RG-AF_Filth_LeavesAspenRedTree</thingToSpawn>
				<spawnCount>2</spawnCount>
				<spawnIntervalRange>60000~120000</spawnIntervalRange>
				<spawnMaxAdjacent>3</spawnMaxAdjacent>
				<saveKeysPrefix>aspenLeaves</saveKeysPrefix>
			</li>
		</comps>
	</ThingDef>
	
