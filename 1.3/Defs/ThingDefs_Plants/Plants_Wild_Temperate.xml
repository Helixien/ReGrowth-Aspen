	<ThingDef ParentName="PlantBase">
		<defName>RG-AF_Plant_Deathbell</defName>
		<label>deathbell</label>
		<description>A purple flower which grows in large clusters. It is said to grow where unfortunate deaths have taken place in the past.</description>
		<statBases>
			<MaxHitPoints>85</MaxHitPoints>
			<Beauty>4</Beauty>
			<Nutrition>0.18</Nutrition>
		</statBases>
		<graphicData>
			<texPath>Things/Plant/Deathbell</texPath>
			<graphicClass>Graphic_Random</graphicClass>
		</graphicData>
		<hideAtSnowDepth>0.6</hideAtSnowDepth>
		<ingestible/>
		<plant>
			<fertilitySensitivity>0.0</fertilitySensitivity>
			<growDays>2.5</growDays>
			<sowTags>
				<li>Ground</li>
			</sowTags>
			<fertilityMin>0.05</fertilityMin>
			<maxMeshCount>9</maxMeshCount>
			<visualSizeRange>0.4~0.6</visualSizeRange>
			<topWindExposure>0.2</topWindExposure>
			<wildClusterRadius>4</wildClusterRadius>
			<wildClusterWeight>8</wildClusterWeight>
			<wildOrder>1</wildOrder>
			<purpose>Beauty</purpose>
		</plant>
	</ThingDef>

	<ThingDef ParentName="PlantBase">
